mixin faq-toggle(datas, cat)
  each data in datas
    if data.cat == cat
      .toggle.faq-toggle.faq-toggle__design
        .toggle__trigger.faq-toggle__trigger
          .faq-toggle__num!=data.num
          .faq-toggle__title.maru-m!=data.title
            i.icon-xs.toggle__trigger-icon.faq-toggle__trigger-icon.material-icons-outlined(data-icon='arrow_forward_ios')
        .toggle__content.faq-toggle__content!=data.content

mixin section-faq()
  section.section.section-faq#section-faq
    .wrap__inner
      .center(data-aos="fade-up" data-aos-duration="1200")
        h2.section-title.maru-b FAQ
      div(data-aos="fade-up" data-aos-duration="1400")
        +faq-toggle(faq_data, '常見問題')
